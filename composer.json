    "homepage": "http://github.com/TheMonkeys/laravel-silverstripe",
    "keywords": ["cms", "silverstripe", "content", "orm", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "themonkeys",
            "email": "developers@themonkeys.com.au"
        }
